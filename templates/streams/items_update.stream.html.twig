{# Learn how to use Turbo Streams: https://github.com/symfony/ux-turbo#broadcast-doctrine-entities-update #}
<turbo-stream action="update" target="item_bag_content_{{ bag.id }}">
    <template>
        {% for itemObject in bag.items%}
            {{ item_object_render(itemObject) }}
        {% endfor %}
    </template>
</turbo-stream>
